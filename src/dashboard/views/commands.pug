include includes/mixins.pug

doctype
html(lang="en")
    head
        include includes/header.pug
    body.bg-dark
        include includes/navbar.pug
        .container.mt-3
            .jumbotron.text-center.bg-transparent
                h1.display-4 Commands
                p.lead View the commands of Mindful.
                hr
            section#commands
                .row
                    .col-sm-3
                        each category in categories
                            +category(category)
                    .col-sm-9
                        each command in commands
                            - console.log("a" + commands)
                            +command(command)
                            
